{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=CardOverflow;Trusted_Connection=True;"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Trace"
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "rolling-logs\\{Date}.txt",
          "restrictedToMinimumLevel": "Error"
        }
      }
    ]
  }
}
